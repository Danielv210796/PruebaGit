@model IEnumerable<PruebaGit.Web.Models.AgregarActividad>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Crear nueva actividad", "Create","AgregarActividads1", new { @class = "navbar-brand text-dark" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Actividad.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID_Paciente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID_Empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actividad_Realizar)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Actividad.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID_Paciente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID_Empleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actividad_Realizar)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
